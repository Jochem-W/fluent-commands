{
  "name": "fluent-commands",
  "version": "2.0.0",
  "author": "Jochem-W (https://jochem.cc/)",
  "license": "AGPL-3.0-or-later",
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "pnpm test && tsup",
    "test": "tsc && node --test --experimental-strip-types"
  },
  "bugs": {
    "url": "https://github.com/Jochem-W/fluent-commands/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jochem-W/fluent-commands.git"
  },
  "engineStrict": true,
  "engines": {
    "node": ">=22"
  },
  "packageManager": "pnpm@10.6.3+sha512.bb45e34d50a9a76e858a95837301bfb6bd6d35aea2c5d52094fa497a467c43f5c440103ce2511e9e0a2f89c3d6071baac3358fc68ac6fb75e2ceb3d2736065e6",
  "devEngines": {
    "runtime": {
      "name": "node",
      "onFail": "warn"
    },
    "packageManager": {
      "name": "pnpm",
      "onFail": "warn"
    }
  },
  "peerDependencies": {
    "discord.js": "^14.18.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@tsconfig/node22": "^22.0.0",
    "@tsconfig/strictest": "^2.0.5",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/node": "^22.13.10",
    "discord.js": "^14.18.0",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "prettier": "^3.5.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.1"
  },
  "prettier": {
    "semi": false,
    "proseWrap": "always",
    "plugins": [
      "prettier-plugin-organize-imports"
    ]
  },
  "tsup": {
    "entry": [
      "src/index.mts"
    ],
    "dts": true,
    "format": "esm",
    "clean": true
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "bufferutil",
      "esbuild",
      "zlib-sync"
    ]
  }
}
